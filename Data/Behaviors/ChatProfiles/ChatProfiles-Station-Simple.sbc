        [BroadcastChatType:Chat]

        [ChatMessages: Potential threats from light-matter organisms detected. Request assistance from nearby starfighter.]
        [ChatAudio:ArcHudGPSNotification1]
        [BroadcastChatType:Chat]
      </Description>
    </EntityComponent>

